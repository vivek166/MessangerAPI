@Path("/message")
public class MessageResource {

	public MessageResource() {
		// TODO Auto-generated constructor stub
	}
	@GET
	@Produces(MediaType.TEXT_PLAIN)
	public String getMessage(){
		
		return "hello world";
	}
}
